{
  "product_name": "Fidelity Guarantee Insurance",
  "product_category": "commercial",
  "questions": [
    {
      "question_text": "Insured Company Name",
      "field_name": "insured_name",
      "field_type": "text",
      "section": "Company Information",
      "order_index": 1,
      "validation_rules": {
        "required": true,
        "minLength": 3,
        "maxLength": 200
      },
      "guidance_text": "Enter your company's legal name as registered with authorities. This name will appear on the policy and should match your incorporation documents for claim validity.",
      "placeholder": "e.g., ABC Retail Ltd"
    },
    {
      "question_text": "Insured Address",
      "field_name": "insured_address",
      "field_type": "textarea",
      "section": "Company Information",
      "order_index": 2,
      "validation_rules": {
        "required": true,
        "minLength": 20,
        "maxLength": 500
      },
      "guidance_text": "Provide complete registered office address including building, street, area, city, state and PIN code. This will be the primary correspondence address for all policy matters.",
      "placeholder": "e.g., 456, Commerce House, Connaught Place, New Delhi - 110001",
      "auto_fill_source": "company_profile",
      "auto_fill_enabled": true
    },
    {
      "question_text": "GST Number",
      "field_name": "gst_number",
      "field_type": "text",
      "section": "Company Information",
      "order_index": 3,
      "validation_rules": {
        "required": true,
        "pattern": "^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Z]{1}Z[0-9A-Z]{1}$",
        "minLength": 15,
        "maxLength": 15
      },
      "guidance_text": "Enter your 15-character GST registration number. This is required for tax compliance and will be used for GST invoicing of your insurance premium.",
      "placeholder": "e.g., 07AABCU9603R1ZM",
      "auto_fill_source": "company_profile",
      "auto_fill_enabled": true
    },
    {
      "question_text": "PAN Number",
      "field_name": "pan_number",
      "field_type": "text",
      "section": "Company Information",
      "order_index": 4,
      "validation_rules": {
        "required": true,
        "pattern": "^[A-Z]{5}[0-9]{4}[A-Z]{1}$",
        "minLength": 10,
        "maxLength": 10
      },
      "guidance_text": "Enter your company's 10-character PAN. This unique identifier is essential for policy issuance and tax deduction certificates if applicable.",
      "placeholder": "e.g., AABCU9603R",
      "auto_fill_source": "company_profile",
      "auto_fill_enabled": true
    },
    {
      "question_text": "Nature of Business",
      "field_name": "nature_of_business",
      "field_type": "select",
      "section": "Company Information",
      "order_index": 5,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Banking & Financial Services",
        "Retail & Trading",
        "Manufacturing",
        "IT & Software Services",
        "Healthcare & Pharmaceuticals",
        "Logistics & Transportation",
        "Hospitality & Tourism",
        "Real Estate & Construction",
        "Education & Training",
        "Professional Services",
        "E-commerce",
        "Others"
      ],
      "guidance_text": "Select your primary business activity. Different industries have varying fraud risk profiles which affects coverage terms and premium calculation.",
      "placeholder": "Select your industry",
      "has_other_option": true,
      "other_field": {
        "field_name": "nature_of_business_other",
        "field_type": "text",
        "placeholder": "Please specify your business type",
        "validation_rules": {
          "required": false,
          "maxLength": 200
        },
        "conditional_logic": {
          "show_if": {
            "field": "nature_of_business",
            "value": "Other (Please specify)"
          }
        }
      },
      "auto_fill_source": "company_profile",
      "auto_fill_enabled": true
    },
    {
      "question_text": "Year of Establishment",
      "field_name": "establishment_year",
      "field_type": "number",
      "section": "Company Information",
      "order_index": 6,
      "validation_rules": {
        "required": true,
        "min": 1850,
        "max": 2025
      },
      "guidance_text": "Enter the year your company was incorporated or established. Longer operational history typically indicates stability and may positively impact premium rates.",
      "placeholder": "e.g., 2010",
      "format": "decimal"
    },
    {
      "question_text": "Annual Turnover (â‚¹)",
      "field_name": "annual_turnover",
      "field_type": "number",
      "section": "Company Information",
      "order_index": 7,
      "validation_rules": {
        "required": true,
        "min": 100000,
        "max": 100000000000
      },
      "guidance_text": "Enter your company's total annual revenue for the last financial year. This helps determine the scale of operations and potential exposure to employee fraud risks.",
      "placeholder": "e.g., 500000000",
      "auto_fill_source": "company_profile",
      "auto_fill_enabled": true,
      "format": "indian_currency"
    },
    {
      "question_text": "Contact Person Name",
      "field_name": "contact_person",
      "field_type": "text",
      "section": "Company Information",
      "order_index": 8,
      "validation_rules": {
        "required": true,
        "minLength": 3,
        "maxLength": 100
      },
      "guidance_text": "Provide the name of the authorized person handling insurance matters. This person will be the primary contact for policy servicing and claims coordination.",
      "placeholder": "e.g., Priya Sharma"
    },
    {
      "question_text": "Contact Mobile Number",
      "field_name": "contact_mobile",
      "field_type": "text",
      "section": "Company Information",
      "order_index": 9,
      "validation_rules": {
        "required": true,
        "pattern": "^[6-9][0-9]{9}$",
        "minLength": 10,
        "maxLength": 10
      },
      "guidance_text": "Enter a 10-digit mobile number for urgent communications. This number will receive SMS alerts about policy status and claim updates.",
      "placeholder": "e.g., 9876543210",
      "auto_fill_source": "company_profile",
      "auto_fill_enabled": true
    },
    {
      "question_text": "Contact Email Address",
      "field_name": "contact_email",
      "field_type": "text",
      "section": "Company Information",
      "order_index": 10,
      "validation_rules": {
        "required": true,
        "pattern": "^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$",
        "maxLength": 100
      },
      "guidance_text": "Provide an official email address that is regularly monitored. All policy documents, renewal notices, and important updates will be sent here.",
      "placeholder": "e.g., finance@abcretail.com",
      "auto_fill_source": "company_profile",
      "auto_fill_enabled": true
    },
    {
      "question_text": "Policy Start Date",
      "field_name": "policy_start_date",
      "field_type": "date",
      "section": "Policy Details",
      "order_index": 11,
      "validation_rules": {
        "required": true
      },
      "guidance_text": "Select the date from when you want the fidelity coverage to begin. For renewals, ensure this matches your current policy expiry to avoid coverage gaps.",
      "placeholder": "e.g., 01/04/2025",
      "policy_extractable": true,
      "extraction_priority": "high"
    },
    {
      "question_text": "Policy End Date",
      "field_name": "policy_end_date",
      "field_type": "date",
      "section": "Policy Details",
      "order_index": 12,
      "validation_rules": {
        "required": true
      },
      "guidance_text": "Select the policy expiry date, typically one year from start date. Annual policies are standard for fidelity guarantee insurance.",
      "placeholder": "e.g., 31/03/2026",
      "policy_extractable": true,
      "extraction_priority": "high"
    },
    {
      "question_text": "Policy Type",
      "field_name": "policy_type",
      "field_type": "radio",
      "section": "Policy Details",
      "order_index": 13,
      "validation_rules": {
        "required": true
      },
      "options": [
        "New Policy",
        "Renewal"
      ],
      "guidance_text": "Select 'New Policy' if purchasing fidelity insurance for the first time or switching insurers. Select 'Renewal' if continuing existing coverage.",
      "placeholder": "Select policy type"
    },
    {
      "question_text": "Previous Insurer Name (if renewal)",
      "field_name": "previous_insurer",
      "field_type": "text",
      "section": "Policy Details",
      "order_index": 14,
      "validation_rules": {
        "required": false,
        "maxLength": 200
      },
      "guidance_text": "If renewing, provide your current insurer's name. This helps in policy porting and understanding your claims history for better terms.",
      "placeholder": "e.g., XYZ General Insurance Co. Ltd",
      "conditional_logic": {
        "show_if": {
          "field": "policy_type",
          "value": "Renewal"
        }
      },
      "policy_extractable": true,
      "extraction_priority": "high"
    },
    {
      "question_text": "Coverage Type Required",
      "field_name": "coverage_type",
      "field_type": "select",
      "section": "Coverage Requirements",
      "order_index": 15,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Individual Policy (Named Employees)",
        "Floater Policy (Position-based)",
        "Blanket Policy (All Employees)",
        "Commercial Crime Policy (Comprehensive)"
      ],
      "guidance_text": "Individual covers specific named employees, Floater covers designated positions regardless of who holds them, Blanket covers all employees. Commercial Crime provides broadest protection including third-party fraud.",
      "placeholder": "Select coverage type",
      "policy_extractable": true,
      "extraction_priority": "high"
    },
    {
      "question_text": "Total Number of Employees",
      "field_name": "total_employees",
      "field_type": "number",
      "section": "Employee Coverage",
      "order_index": 16,
      "validation_rules": {
        "required": true,
        "min": 1,
        "max": 100000
      },
      "guidance_text": "Enter the total number of employees in your organization. This helps assess the scale of exposure even if not all employees will be covered.",
      "placeholder": "e.g., 150",
      "format": "decimal"
    },
    {
      "question_text": "Number of Employees to be Covered",
      "field_name": "employees_to_cover",
      "field_type": "number",
      "section": "Employee Coverage",
      "order_index": 17,
      "validation_rules": {
        "required": true,
        "min": 1,
        "max": 100000
      },
      "guidance_text": "Specify how many employees need fidelity coverage. Typically includes those handling cash, inventory, or having financial authority. May be less than total employees.",
      "placeholder": "e.g., 25",
      "format": "decimal"
    },
    {
      "question_text": "Employee Categories to Cover",
      "field_name": "employee_categories",
      "field_type": "multiselect",
      "section": "Employee Coverage",
      "order_index": 18,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Senior Management/Directors",
        "Finance & Accounts Staff",
        "Cashiers/Tellers",
        "Store/Warehouse Staff",
        "Sales Personnel",
        "Purchase Department",
        "IT System Administrators",
        "HR Personnel",
        "Operations Staff",
        "All Employees"
      ],
      "guidance_text": "Select all employee categories requiring coverage. Focus on roles with access to cash, valuable inventory, financial systems, or confidential data.",
      "placeholder": "Select all applicable categories"
    },
    {
      "question_text": "Sum Insured Required Per Employee",
      "field_name": "sum_insured_per_employee",
      "field_type": "number",
      "section": "Employee Coverage",
      "order_index": 19,
      "validation_rules": {
        "required": true,
        "min": 0,
        "max": 100000000000
      },
      "guidance_text": "Select the maximum coverage amount per employee or incident. Should reflect the maximum potential loss from a single employee's fraudulent act. Higher positions may need higher limits. You can enter the amount in lakhs or crores (e.g., 50,00,000 or 5,00,00,000).",
      "placeholder": "Select sum insured",
      "format": "indian_currency",
      "policy_extractable": true,
      "extraction_priority": "high"
    },
    {
      "question_text": "Aggregate Annual Limit Required",
      "field_name": "aggregate_limit",
      "field_type": "number",
      "section": "Employee Coverage",
      "order_index": 20,
      "validation_rules": {
        "required": true,
        "min": 500000,
        "max": 1000000000
      },
      "guidance_text": "Enter the maximum total amount payable for all claims during the policy period. This caps the insurer's liability regardless of the number of incidents.",
      "placeholder": "e.g., 10000000",
      "format": "indian_currency"
    },
    {
      "question_text": "Employee Details Schedule",
      "field_name": "employee_schedule",
      "field_type": "file",
      "section": "Employee Coverage",
      "order_index": 21,
      "validation_rules": {
        "required": false
      },
      "guidance_text": "Upload Excel/PDF with employee names, designations, and sum insured for each. Required for Individual policies, optional for Floater/Blanket policies.",
      "placeholder": "Upload Excel/PDF file",
      "conditional_logic": {
        "show_if": {
          "field": "coverage_type",
          "value": "Individual Policy (Named Employees)"
        }
      }
    },
    {
      "question_text": "Pre-Employment Background Verification Done?",
      "field_name": "background_verification",
      "field_type": "radio",
      "section": "Risk Management",
      "order_index": 22,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Yes",
        "No"
      ],
      "guidance_text": "Indicates if you verify employee credentials, criminal records, and references before hiring. Strong verification processes reduce fraud risk and may lower premiums.",
      "placeholder": "Select Yes or No"
    },
    {
      "question_text": "Background Verification Process",
      "field_name": "verification_process",
      "field_type": "multiselect",
      "section": "Risk Management",
      "order_index": 23,
      "validation_rules": {
        "required": false
      },
      "options": [
        "Identity & Address Verification",
        "Education Verification",
        "Employment History Check",
        "Criminal Record Check",
        "Credit History Check",
        "Reference Checks",
        "Drug Testing",
        "Professional Agency Used"
      ],
      "guidance_text": "Select all verification methods used. Comprehensive background checks demonstrate strong risk management and can positively impact coverage terms.",
      "placeholder": "Select all applicable methods",
      "conditional_logic": {
        "show_if": {
          "field": "background_verification",
          "value": "Yes"
        }
      }
    },
    {
      "question_text": "Dual Control/Maker-Checker Process?",
      "field_name": "dual_control",
      "field_type": "radio",
      "section": "Risk Management",
      "order_index": 24,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Yes",
        "No"
      ],
      "guidance_text": "Indicates if critical transactions require approval from two different employees. This segregation of duties significantly reduces fraud opportunity and is viewed favorably by insurers.",
      "placeholder": "Select Yes or No"
    },
    {
      "question_text": "Areas with Dual Control",
      "field_name": "dual_control_areas",
      "field_type": "multiselect",
      "section": "Risk Management",
      "order_index": 25,
      "validation_rules": {
        "required": false
      },
      "options": [
        "Cash Handling",
        "Bank Transfers",
        "Vendor Payments",
        "Salary Processing",
        "Inventory Management",
        "Purchase Orders",
        "System Access Controls",
        "Financial Reporting"
      ],
      "guidance_text": "Select all areas where dual control is implemented. More areas under dual control indicate stronger internal controls and reduced fraud risk.",
      "placeholder": "Select all applicable areas",
      "conditional_logic": {
        "show_if": {
          "field": "dual_control",
          "value": "Yes"
        }
      }
    },
    {
      "question_text": "Frequency of Internal Audits",
      "field_name": "audit_frequency",
      "field_type": "select",
      "section": "Risk Management",
      "order_index": 26,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Monthly",
        "Quarterly",
        "Half-Yearly",
        "Annually",
        "No Regular Audits"
      ],
      "guidance_text": "Indicate how often internal audits are conducted. Regular audits help detect fraud early and demonstrate proactive risk management to insurers.",
      "placeholder": "Select audit frequency"
    },
    {
      "question_text": "Type of Audits Conducted",
      "field_name": "audit_types",
      "field_type": "multiselect",
      "section": "Risk Management",
      "order_index": 27,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Financial Audits",
        "Operational Audits",
        "Compliance Audits",
        "IT System Audits",
        "Surprise Cash Counts",
        "Inventory Verification",
        "Vendor Audits",
        "External Audits"
      ],
      "guidance_text": "Select all types of audits performed. Comprehensive audit coverage across different areas indicates robust control environment and reduces fraud risk.",
      "placeholder": "Select all audit types"
    },
    {
      "question_text": "Employee Access to Cash/Valuables",
      "field_name": "cash_access",
      "field_type": "select",
      "section": "Risk Management",
      "order_index": 28,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Very Limited (< 5 employees)",
        "Limited (5-10 employees)",
        "Moderate (11-25 employees)",
        "Extensive (26-50 employees)",
        "Very Extensive (> 50 employees)"
      ],
      "guidance_text": "Indicate how many employees have access to cash, valuable inventory, or negotiable instruments. Limiting access reduces exposure to fraud losses.",
      "placeholder": "Select access level"
    },
    {
      "question_text": "Maximum Cash/Valuables Exposure",
      "field_name": "max_cash_exposure",
      "field_type": "number",
      "section": "Risk Management",
      "order_index": 29,
      "validation_rules": {
        "required": true,
        "min": 0,
        "max": 100000000
      },
      "guidance_text": "Enter the maximum value of cash or valuables accessible to any single employee at one time. This helps determine appropriate coverage limits and assess risk concentration.",
      "placeholder": "e.g., 5000000",
      "format": "decimal"
    },
    {
      "question_text": "IT Systems and Access Controls",
      "field_name": "it_controls",
      "field_type": "multiselect",
      "section": "Risk Management",
      "order_index": 30,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Role-based Access Control",
        "Regular Password Changes",
        "Multi-factor Authentication",
        "Activity Logs/Audit Trails",
        "Automated Alerts for Unusual Transactions",
        "Regular Access Reviews",
        "Data Encryption",
        "Segregation in ERP Systems"
      ],
      "guidance_text": "Select all IT security measures implemented. Strong system controls prevent unauthorized access and fraudulent transactions, reducing cyber-related fraud risks.",
      "placeholder": "Select all implemented controls"
    },
    {
      "question_text": "Any Claims in Last 5 Years?",
      "field_name": "claims_history",
      "field_type": "radio",
      "section": "Claims History",
      "order_index": 31,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Yes",
        "No"
      ],
      "guidance_text": "Declare any fidelity guarantee or employee dishonesty claims made in the past 5 years. Full disclosure ensures proper coverage and avoids claim disputes.",
      "placeholder": "Select Yes or No"
    },
    {
      "question_text": "Claims Details",
      "field_name": "claims_details",
      "field_type": "textarea",
      "section": "Claims History",
      "order_index": 32,
      "validation_rules": {
        "required": false,
        "maxLength": 1000
      },
      "guidance_text": "Provide details including date, nature of fraud, amount involved, and corrective actions taken. Past claims help insurers assess risk but don't automatically increase premiums if controls improved.",
      "placeholder": "e.g., March 2023 - Cashier embezzlement of â‚¹8 lakhs, claim settled, enhanced controls implemented",
      "conditional_logic": {
        "show_if": {
          "field": "claims_history",
          "value": "Yes"
        }
      }
    },
    {
      "question_text": "Known Losses Not Yet Claimed?",
      "field_name": "known_losses",
      "field_type": "radio",
      "section": "Claims History",
      "order_index": 33,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Yes",
        "No"
      ],
      "guidance_text": "Declare if you're aware of any employee fraud that hasn't been claimed yet. New policies typically don't cover known losses, so disclosure is crucial.",
      "placeholder": "Select Yes or No"
    },
    {
      "question_text": "Discovery Period Required?",
      "field_name": "discovery_period",
      "field_type": "radio",
      "section": "Additional Coverage",
      "order_index": 34,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Yes",
        "No"
      ],
      "guidance_text": "Discovery period allows claiming for frauds that occurred during the policy period but are discovered after expiry. Typically 6-12 months, useful as fraud often takes time to detect.",
      "placeholder": "Select Yes or No"
    },
    {
      "question_text": "Discovery Period Duration",
      "field_name": "discovery_period_months",
      "field_type": "select",
      "section": "Additional Coverage",
      "order_index": 35,
      "validation_rules": {
        "required": false
      },
      "options": [
        "6 Months",
        "12 Months",
        "24 Months"
      ],
      "guidance_text": "Select the extended discovery period duration. Longer periods provide more time to detect and claim for frauds but increase premium cost.",
      "placeholder": "Select duration",
      "conditional_logic": {
        "show_if": {
          "field": "discovery_period",
          "value": "Yes"
        }
      }
    },
    {
      "question_text": "Retroactive Date Coverage Required?",
      "field_name": "retroactive_coverage",
      "field_type": "radio",
      "section": "Additional Coverage",
      "order_index": 36,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Yes",
        "No"
      ],
      "guidance_text": "Covers frauds that occurred before policy start but are discovered during the policy period. Important for first-time buyers or when switching insurers.",
      "placeholder": "Select Yes or No"
    },
    {
      "question_text": "Computer Crime Coverage Required?",
      "field_name": "computer_crime_coverage",
      "field_type": "radio",
      "section": "Additional Coverage",
      "order_index": 37,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Yes",
        "No"
      ],
      "guidance_text": "Extends coverage to include frauds through computer systems, hacking, or electronic fund transfers. Essential in today's digital business environment.",
      "placeholder": "Select Yes or No"
    },
    {
      "question_text": "Third Party Coverage Required?",
      "field_name": "third_party_coverage",
      "field_type": "radio",
      "section": "Additional Coverage",
      "order_index": 38,
      "validation_rules": {
        "required": true
      },
      "options": [
        "Yes",
        "No"
      ],
      "guidance_text": "Covers losses from fraudulent acts by third parties like vendors, customers, or service providers. Broader than standard fidelity coverage which only covers employees.",
      "placeholder": "Select Yes or No"
    },
    {
      "question_text": "Deductible Preference",
      "field_name": "deductible_amount",
      "field_type": "number",
      "section": "Policy Terms",
      "order_index": 39,
      "validation_rules": {
        "required": true,
        "min": 0,
        "max": 100000000000
      },
      "guidance_text": "Select the amount you'll bear per claim before insurance pays. Higher deductibles reduce premium but increase your out-of-pocket expense during claims. You can enter the amount in lakhs or crores (e.g., 50,00,000 or 5,00,00,000).",
      "placeholder": "Select deductible",
      "format": "indian_currency",
      "policy_extractable": true,
      "extraction_priority": "high"
    },
    {
      "question_text": "Upload Previous Policy",
      "field_name": "previous_policy_file",
      "field_type": "file",
      "section": "Supporting Documents",
      "order_index": 40,
      "validation_rules": {
        "required": false
      },
      "guidance_text": "If renewing, upload your current/expiring policy document. This helps understand existing coverage and ensures seamless continuation.",
      "placeholder": "Upload PDF file",
      "conditional_logic": {
        "show_if": {
          "field": "policy_type",
          "value": "Renewal"
        }
      }
    },
    {
      "question_text": "Upload Audited Financial Statements",
      "field_name": "financial_statements_file",
      "field_type": "file",
      "section": "Supporting Documents",
      "order_index": 41,
      "validation_rules": {
        "required": false
      },
      "guidance_text": "Upload last 2 years' audited financials. These help insurers assess financial stability and determine appropriate coverage limits.",
      "placeholder": "Upload PDF file",
      "auto_fill_source": "company_profile",
      "auto_fill_enabled": true
    },
    {
      "question_text": "Upload HR/Admin Policy Manual",
      "field_name": "hr_policy_file",
